{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Extension Rebuild",
  "description": "Extension Rebuild",
  "permissions": ["tabs", "activeTab", "storage"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["src/content-scripts/ApplyToSupply.tsx"],
      "matches": ["https://www.applytosupply.digitalmarketplace.service.gov.uk/*"]
    }
  ]
}